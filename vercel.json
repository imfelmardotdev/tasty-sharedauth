{
  "version": 2,
  "routes": [
    {
      "src": "/api/get-totp",
      "methods": ["POST"],
      "dest": "/api/get-totp.ts"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/(.+\\.(svg|ico|png|jpg|jpeg|gif))$",
      "dest": "/public/$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
